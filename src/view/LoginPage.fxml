      <Button layoutX="22.0" layoutY="112.0" mnemonicParsing="false" onAction="#LogIn" text="Log in" />
      <Button layoutX="120.0" layoutY="112.0" mnemonicParsing="false" onAction="#SignUp" text="Sign up" />
   </children>
</AnchorPane>
